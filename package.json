{
  "name": "express-graphql-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate template for express-graphql server",
  "main": "src/index.js",
  "scripts": {
    "dev": "eval $(cat .env) && nodemon -r esm",
    "start": "NODE_ENV=production node -r esm",
    "clean:install": "rm -rf node_modules && yarn install"
  },
  "keywords": [
    "Express",
    "GraphQL",
    "Apollo",
    "Node"
  ],
  "author": "Ashiru Olawale",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^2.19.0",
    "bcryptjs": "^2.4.3",
    "consola": "^2.15.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.13",
    "mongoose-paginate-v2": "^1.3.11",
    "nodemailer": "^6.4.15",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  },
  "engines": {
    "node": ">= 12.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/walebant/apollo-express-graphql-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/walebant/apollo-express-graphql-boilerplate/issues"
  }
}
